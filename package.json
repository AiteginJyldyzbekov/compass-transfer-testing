{
  "name": "compass",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.4.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [
    "nextjs"
  ],
  "author": "Valentin Krivich",
  "license": "MIT",
  "description": "Compass Traansfer KYRGYZSTAN",
  "scripts": {
    "dev:admin": "yarn workspace compass-admin dev",
    "dev:terminal": "yarn workspace compass-terminal dev",
    "dev:driver": "yarn workspace compass-driver dev",
    "dev:driver-mobile": "yarn workspace compass-driver-mobile dev",
    "build:admin": "yarn workspace compass-admin build",
    "build:terminal": "yarn workspace compass-terminal build",
    "build:driver": "yarn workspace compass-driver build",
    "build:driver-mobile": "yarn workspace compass-driver-mobile build",
    "start:admin": "yarn workspace compass-admin start",
    "start:terminal": "yarn workspace compass-terminal start",
    "start:driver": "yarn workspace compass-driver start",
    "start:driver-mobile": "yarn workspace compass-driver-mobile start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --format=stylish --max-warnings=0 --cache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --format=stylish --cache",
    "lint:watch": "nodemon --watch \"**/*.{js,jsx,ts,tsx}\" --exec \"yarn lint\"",
    "lint:debug": "eslint . --ext .js,.jsx,.ts,.tsx --format=stylish --debug",
    "lint:report": "node scripts/lint-report.js",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md}\"",
    "fix-imports": "eslint --fix \"**/*.{ts,tsx}\" --rule \"import/order: error\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "build:check": "yarn type-check && yarn build",
    "generate-api-docs": "node scripts/generate-api-docs.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.0.1",
    "@microsoft/signalr": "^8.0.7",
    "@pbe/react-yandex-maps": "^1.2.5",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@swc/core": "^1.11.24",
    "@swc/helpers": "^0.5.17",
